<project name="Ubuntu:%release%">
  <title>Ubuntu %release% Release (%codename%)</title>
  <description>This contains all packages from Ubuntu %release% "main", this includes also the packages from KUbuntu, XUbuntu and Edubuntu.</description>
  <person userid="luksoft" role="maintainer"/>
  <person userid="Admin" role="maintainer"/>
  <build>
    <enable/>
  </build>
  <publish>
    <enable/>
  </publish>
  <debuginfo>
    <disable/>
  </debuginfo>
  <binarydownload>
    <enable/>
  </binarydownload>
  <sourceaccess>
    <disable/>
  </sourceaccess>
  <access>
    <enable/>
  </access>

  <download baseurl="http://mirror.vutbr.cz/ubuntu/archive" metafile="Packages" mtype="debmd" arch="x86_64" />
