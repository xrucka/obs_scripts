-->
  <repository name="multiverse">
    <arch>x86_64</arch>
  </repository>
  <repository name="universe">
    <arch>x86_64</arch>
  </repository>
  <repository name="main">
    <arch>x86_64</arch>
  </repository>
  <repository name="restricted">
    <arch>x86_64</arch>
  </repository>
</project>
